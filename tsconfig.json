{
  "compilerOptions": {
    "jsxImportSource": "nativewind",
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "types": ["nativewind/types"],
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "nativewind.d.ts",
    "src",
    "App.tsx",
    "nativewind-env.d.ts"
, "src/screens", "app/(auth)/OTP.tsx", "app/(onboarding)/ProfileName.tsx", "app/(onboarding)/Interests.tsx", "app/(onboarding)/Gender.tsx", "app/index.tsx", "components"  ],
  "extends": "expo/tsconfig.base"
}